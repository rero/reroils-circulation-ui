{
  "name": "reroils-circulation-ui",
  "version": "0.1.0a17",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --locale fr",
    "build": "ng build --prod",
    "buildprod": "ng build --aot -prod --deploy-url=/static/js/reroils_data/circulation_ui --output-hashing=none --extract-css=false",
    "builden": "npm run buildprod --  --output-path build/en --locale en",
    "postbuilden": "mv build/en/main.bundle.js build/en/main.bundle_en.js",
    "buildfr": "npm run buildprod --  --output-path build/fr --locale fr",
    "postbuildfr": "mv build/fr/main.bundle.js build/fr/main.bundle_fr.js",
    "buildit": "npm run buildprod --  --output-path build/it --locale it",
    "postbuildit": "mv build/it/main.bundle.js build/it/main.bundle_it.js",
    "buildde": "npm run buildprod --  --output-path build/de --locale de",
    "postbuildde": "mv build/de/main.bundle.js build/de/main.bundle_de.js",
    "ctest": "ng test --browsers Chrome",
    "ftest": "ng test --browsers Firefox",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "proxy": "ng serve --aot --locale fr --proxy-config proxy.conf.json -e prod",
    "extract_fixtures": "./scripts/load_fixtures_from_invenio.py > src/app/fixtures.json",
    "extract_messages": "ngx-translate-extract -i ./src -o ./src/assets/i18n/en*.json -m _; key2value.py ./src/assets/i18n/en_US.json; key2value.py ./src/assets/i18n/en.json",
    "update_catalog": "ngx-translate-extract -i ./src -o ./src/assets/i18n/{it,de,en,fr}.json -m _",
    "buildall": "npm run builden; npm run buildfr; npm run buildde; npm run buildit",
    "deploy": "npm run buildall; cp build/*/main.bundle*.js build/en/*.js",
    "reroilsdeploy": "npm run deploy -- ../reroils-data/reroils_data/items/static/js/reroils_data/circulation_ui",
    "clean": "rm -fr build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.11",
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-datatables": "^6.0.0",
    "core-js": "^2.5.6",
    "datatables.net": "^1.10.16",
    "datatables.net-bs": "^1.10.16",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "rxjs": "^5.5.11",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/language-service": "^5.2.11",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/datatables.net": "^1.10.11",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.111",
    "angular-in-memory-web-api": "^0.5.4",
    "angular2-uuid": "^1.1.1",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-jquery": "^0.2.2",
    "ngx-bootstrap": "^2.0.5",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
